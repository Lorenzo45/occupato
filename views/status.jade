extends layout

block content
  .statuses.fill
    .container.fill
      .row.fill
        .col-sm-12.fill
          div(class="col-sm-4 occupato-status occupato-mens occupato-#{ mens }")
            .fa.fa-male.mens-logo.dimensions.vertical-align


          div(class="col-sm-4 occupato-status occupato-womens occupato-#{ womens }")
            .fa.fa-female.womens-logo.dimensions


          div(class="col-sm-4 occupato-status occupato-shower occupato-#{ shower }")
            .fa.fa-cloud-download.shower-logo.dimensions

